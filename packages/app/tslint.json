{
  "extends": [
    "tslint-config-station",
    "tslint-react"
  ],
  "rules": {
    "jsx-no-lambda": [true, {}],
    "jsx-no-bind": [true, {}],
    "jsx-self-close": [true, {}],
    "jsx-no-multiline-js": false,
    "import-blacklist": false
  },
  "linterOptions": {
    "exclude": [
      "**/*generated.ts?(x)",
      "**/*.json"
    ]
  }
}
